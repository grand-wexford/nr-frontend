<div class="print-holder no-mobile">
	<button type="button" class="btn btn-default btn-xs print"><span class="glyphicon glyphicon-print " aria-hidden="true"></span> <%=text.print%></button>
</div>
<div class="recipe-title"><%=title%></div>
<div class='recipe-list-holder'>
	<div class='recipe-header'>
		<span class="recipe-header-row"><span class="recipe-header-row-title"><%=text.recipe_FIO_patient%>: </span><span  class="recipe-patient-name"><input class="recipe-patient-fio" type="text" value="" tabindex="1"></span></span>
		<span class="recipe-header-row recipe-row-separator"><span class="recipe-header-row-title"><%=text.recipe_age%>: </span><span  class="recipe-patient-name"><input class="recipe-age" type="text" value="<%=data.age%>" tabindex="2"></span></span>

		<span class="recipe-header-row"><span class="recipe-header-row-title"><%=text.recipe_FIO_doctor%>: </span><span  class="recipe-medic-name"><input class="recipe-doctor-fio" type="text" value="" tabindex="3"></span></span>
		<span class="recipe-header-row"><span class="recipe-header-row-title"><%=text.recipe_LPU%>: </span><span  class="recipe-medic-name"><input class="recipe-lpu" type="text" value="" tabindex="4"></span></span>
	</div>
	<div class='recipe-list'><%=result%></div>
</div>
<div class='recipe-bottom'>
	<span class="recipe-sign"><span class="recipe-sign-text"><%=text.sign%></span></span>
	<span class="recipe-mp"><%=text.locus_sigilli%></span>
	<span class="recipe-data-holder"><span class="recipe-header-row-title"><%=text.date%></span> <span  class="recipe-patient-name recipe-date"><%=data.date%></span></span>

	<div class="recipe-buttons">
		<div class="btn-group btn-group-justified" role="group" aria-label="">
			<div class="btn-group" role="group">
				<button type="button" class="btn btn-sm btn-primary to-mnn"><%=text.to_mnn%></button>
			</div>
		</div>
	</div>							

	<span class="recipe-copyright">Pharm Expert<sup>&reg;</sup><BR>Socmedica (www.socmedica.com)</span>
</div>