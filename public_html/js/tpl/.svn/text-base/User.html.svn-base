<div class="user-title"></div>
<div class='user-text'>
	<div class="streak">
		<div class="row ">
			<div class="col-md-12">
				<span class="input-title input-40"><span><%=text.age%>:</span></span>
				<span class="input-values">
					<input class="data-input age" type="text" value="" name="age" maxlength="4" >
					<select class="data-select age_dwmy" name="age_dwmy">
						<option value='d' class="age_dwmy_d"><%=text.cases_d_2%></option>
						<option value='w' class="age_dwmy_w"><%=text.cases_w_2%></option>
						<option value='m' class="age_dwmy_m"><%=text.cases_m_2%></option>
						<option value='y' class="age_dwmy_y" selected="selected"><%=text.cases_y_2%></option>
					</select>
				</span>
			</div>
		</div>
	</div>

	<div class="streak">
		<div class="row ">
			<div class="col-md-12">
				<span class="input-title input-40"><span><%=text.sex%>:</span></span>
				<span class="input-values ">
					<label class="radio-inline"><input type="radio" class="sex" name="sex" value="m"><%=text.male%></label>
					<label class="radio-inline"><input type="radio" class="sex" name="sex" value="f"><%=text.female%></label>
				</span>
			</div>
		</div>
	</div>
	<div class="streak">
		<div class="row ">
			<div class="col-md-12">
				<span class="input-title input-40"><span><%=text.growth%>:</span></span>
				<span class="input-values ">
					<input class="data-input growth" type="text" value="" name="growth" maxlength="3" > <%=text.sm%>
				</span>
			</div>
		</div>
	</div>
	<div class="streak">
		<div class="row ">
			<div class="col-md-12">
				<span class="input-title input-40"><span><%=text.weight%>:</span></span>
				<span class="input-values ">
					<input class="data-input weight" type="text" value="" name="weight" maxlength="3" > <%=text.kg%>
				</span>
			</div>
		</div>
	</div>
